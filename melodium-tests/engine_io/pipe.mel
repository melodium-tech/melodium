
use std/process/engine::Read
use std/process/engine::Write
use std/fs/write/mono::WritePath
use std/text/encoding::Encode

sequence WriteFile(const file: string)
  input text: Stream<string>
{

    Encode()
    WritePath(path=file)
    
    Self.text -> Encode.text,data -> WritePath.data
}

sequence Main()
{
    Read()
    Write()
    WriteFile(file="output.txt")

    Read.line -> Write.text
    Read.line -> WriteFile.text
}



use core/comparison/scalar/i128::GreaterEqual as CoreGreaterEqual

/**
Tells whether `a` is greater or equal to `b`.
*/
sequence GreaterEqual()
  input a: Stream<i128>
  input b: Stream<i128>
  output is: Stream<bool>
{
    CoreGreaterEqual()

    Self.a -> CoreGreaterEqual.a
    Self.b -> CoreGreaterEqual.b,is -> Self.is
}


use core/comparison/scalar/i128::LowerEqual as CoreLowerEqual

/**

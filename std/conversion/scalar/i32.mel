use core/conversion/scalar::I32ToVoid

/**
Turns `i32` stream into `void` one.

Send one `iter` per input `value` received.
*/
sequence ToVoid()
  input value: Stream<i32>
  output iter: Stream<void>
{
    I32ToVoid()

    Self.value -> I32ToVoid.value,iter -> Self.iter
}

use core/conversion/scalar::I32ToByte

/**
Convert stream of `i32` into `byte`.

`i32` gets converted into `byte`, and the resulting bytes are send through stream in continuity.
*/
sequence ToByte()
  input  value: Stream<i32>
  output data:  Stream<byte>
{
    I32ToByte()

    Self.value -> I32ToByte.value,data -> Self.data
}

use core/conversion/scalar::I32ToString

/**

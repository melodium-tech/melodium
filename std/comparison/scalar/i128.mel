use core/comparison/scalar/i128::Equal as CoreEqual

/**
Tells whether `a` is equal to `b`.
*/
sequence Equal()
  input a: Stream<i128>
  input b: Stream<i128>
  output is: Stream<bool>
{
    CoreEqual()

    Self.a -> CoreEqual.a
    Self.b -> CoreEqual.b,is -> Self.is
}


